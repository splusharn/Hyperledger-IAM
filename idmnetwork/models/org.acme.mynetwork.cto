namespace org.acme.mynetwork

participant Person identified by uid {
  o String uid
  o String firstName
  o String lastName
}

asset Account identified by eruid {
  o String eruid
  --> Person owner
  o String value
}

transaction SampleTransaction {
  --> Account asset
  o String newValue
}

event SampleEvent {
  --> Account asset
  o String oldValue
  o String newValue
}
